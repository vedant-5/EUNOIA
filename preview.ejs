<body>
  <%- include('./partials/navbar') -%>
    
        <!--END OF HEADER-->
    

  <h1 align="center">CHECK YOUR DETAILS HERE:</h1>
  <table align="center">

    <br><br>

    <tr>
      <th>FIRST NAME</th>
      <td><%= first_name %></td>
    </tr>

    <tr>
      <th>LAST NAME</th>
      <td><%= last_name %></td>
    </tr>

    <tr>
      <th>EMAIL ID</th>
      <td><%= email %></td>
    </tr>

    <tr>
      <th>PHONE NUMBER</th>
      <td><%= phone %></td>
    </tr>

    <!--INSERT IMAGE HERE-->

    <tr>
      <th>REGISTRATION TYPE</th>
      <td><%= subject %></td>
    </tr>

    <tr>
      <th>NUMBER OF TICKETS</th>
      <td><%= tickets%></td>
    </tr>

  </table>

  <a href="/forms"<button class="edit" type="submit">EDIT</button></a>

  <a href="/success"><button class="submit" type="submit">SUBMIT</button></a>


